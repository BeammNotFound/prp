<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.PopularizationsMapper">
    <select id="queryPopularizationsList" resultType="com.pojo.Popularizations">
        select *
        from Popularizations;
    </select>

    <insert id="createPopularization" parameterType="com.pojo.Popularizations">
        insert into
        Popularizations(p_title,p_details,p_author,p_pv,p_createtime,p_updatetime,p_image)
        values (#{p_title},#{p_details},#{p_author},#{p_pv},#{p_createtime},#{p_updatetime},#{p_image})
    </insert>
</mapper>